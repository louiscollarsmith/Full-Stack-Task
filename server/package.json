{
  "name": "ifd-kibana-proxy",
  "version": "1.0.0",
  "description": "An IDF proxy server to proxy to kibana UI ",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm run clean && node_modules/.bin/tsc",
    "build:watch": "npm run build -- -w",
    "start:dev": "npm run build:watch | NODE_OPTIONS=--enable-source-maps nodemon --inspect ./build/server.js",
    "start": "npm run build && NODE_OPTIONS=--enable-source-maps node ./build/server.js",
    "lint": "eslint 'src/**/*.ts' --fix"
  },
  "author": "Abhijit Baldawa",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.21",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.6",
    "rimraf": "^3.0.2",
    "winston": "^3.3.3"
  }
}
